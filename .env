# Enable WebSocket for real-time streaming
USE_WEBSOCKET=true

# Separate URLs for HTTP and WebSocket endpoints
AGENT_HTTP_URL=http://localhost:8080
AGENT_WS_URL=ws://localhost:8081

# Logging
RUST_LOG=info,a2a_client=debug,a2a_rs=debug,a2a_agents=debug

# Persistent storage (SQLite with auto-create)
